{
  "name": "angular",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build": "node_modules/.bin/tsc -p src/",
    "build:watch": "node_modules/.bin/tsc -p src/ -w",
    "build:e2e": "node_modules/.bin/tsc -p e2e/",
    "serve": "node_modules/.bin/lite-server -c=bs-config.json",
    "serve:e2e": "node_modules/.bin/lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "node_modules/.bin/concurrently \\\"npm run build:watch\\\" \\\"npm run serve\\\"",
    "pree2e": "npm run build:e2e",
    "e2e": "node_modules/.bin/concurrently \\\"npm run serve:e2e\\\" \\\"npm run protractor\\\" --kill-others --success first",
    "preprotractor": "node_modules/.bin/webdriver-manager update",
    "protractor": "node_modules/.bin/protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "node_modules/.bin/concurrently \\\"npm run build:watch\\\" \\\"karma start karma.conf.js\\\"",
    "pretest:once": "npm run build",
    "test:once": "node_modules/.bin/karma start karma.conf.js --single-run",
    "lint": "node_modules/.bin/tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.3.4",
    "@angular/compiler": "~4.3.4",
    "@angular/core": "~4.3.4",
    "@angular/forms": "~4.3.4",
    "@angular/http": "~4.3.4",
    "@angular/platform-browser": "~4.3.4",
    "@angular/platform-browser-dynamic": "~4.3.4",
    "@angular/router": "~4.3.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "angular-font-awesome": "^3.1.2",
    "angular-in-memory-web-api": "~0.3.0",
    "bootstrap": "^5.0.0-beta3",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "npm-watch": "^0.9.0",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "jasmine-core": "~2.4.1",
    "karma": "^6.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.6.1",
    "lodash": "^4.16.4",
    "protractor": "^7.0.0",
    "rimraf": "^2.5.4",
    "tslint": "^5.20.1",
    "typescript": "~2.1.0"
  },
  "repository": {}
}
